{
  "name": "Unsplash",
  "auth": {
    "authType": "OAUTH2",
    "tokenParams": {},
    "authorizationParams": {},
    "authorizationURL": "https://unsplash.com/oauth/authorize",
    "tokenURL": "https://unsplash.com/oauth/token"
  },
  "request": {
    "headers": {
      "Accept": "application/json",
      "User-Agent": "Pizzly",
      "Authorization": "Bearer ${auth.accessToken}"
    },
    "baseURL": "https://api.unsplash.com/"
  }
}
